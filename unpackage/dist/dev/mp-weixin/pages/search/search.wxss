.search-container .history-box {
  padding: 25rpx 30rpx 0 20rpx;
}
.search-container .history-box .top {
  display: flex;
  justify-content: space-between;
}
.search-container .history-box .bottom {
  display: flex;
  flex-wrap: wrap;
  margin-top: 30rpx;
}
.search-container .history-box .bottom view {
  padding: 20rpx;
  margin-right: 30rpx;
  margin-bottom: 20rpx;
  background-color: #dddddd;
}

