.search-container .search-box {
  height: 120rpx;
  background-color: #eeeeee;
  padding: 30rpx 20rpx 30rpx 10rpx;
  box-sizing: border-box;
  position: relative;
  display: flex;
  justify-content: space-between;
}
.search-container .search-box icon {
  position: absolute;
  top: 38rpx;
  left: 30rpx;
}
.search-container .search-box input {
  background-color: white;
  padding-left: 70rpx;
  border-radius: 4rpx;
  margin-right: 20rpx;
  flex: 1;
  height: 60rpx;
}
.search-container .search-box button {
  width: 160rpx;
  height: 60rpx;
  text-align: center;
  line-height: 60rpx;
  border-radius: 4rpx;
}
.search-container .history-box {
  padding: 25rpx 30rpx 0 20rpx;
}
.search-container .history-box .top {
  display: flex;
  justify-content: space-between;
}
.search-container .history-box .bottom {
  display: flex;
  flex-wrap: wrap;
  margin-top: 30rpx;
}
.search-container .history-box .bottom view {
  padding: 20rpx;
  margin-right: 30rpx;
  margin-bottom: 20rpx;
  background-color: #dddddd;
}

