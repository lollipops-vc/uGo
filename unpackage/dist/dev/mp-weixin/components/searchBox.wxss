.search-box {
  height: 120rpx;
  background-color: #eeeeee;
  padding: 30rpx 20rpx 30rpx 10rpx;
  box-sizing: border-box;
  position: relative;
  display: flex;
  justify-content: space-between;
}
.search-box icon {
  position: absolute;
  top: 38rpx;
  left: 30rpx;
}
.search-box input {
  background-color: white;
  padding-left: 70rpx;
  border-radius: 4rpx;
  margin-right: 20rpx;
  flex: 1;
  height: 60rpx;
}
.search-box button {
  width: 160rpx;
  height: 60rpx;
  text-align: center;
  line-height: 60rpx;
  border-radius: 4rpx;
}

